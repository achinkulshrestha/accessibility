{
	"manifest_version": 2,
	"name": "Project Vibl",
	"description": "A too to help web apps more accessible to the blind and visually impaired",
	"version": "1.0",
	"devtools_page": "devtools.html",
	"permissions": [
		"storage",
		"tabs"
	],
	"content_scripts": [
		{
		"matches": [
				"http://*/*",
				"https://*/*"
				],
		"js": ["axs_testing.js"]
		}
	],
	"browser_action": {
		"default_icon": {
			"19": "img/icon48.png",
			"38": "img/icon48.png"
		},
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"]
	},
	 "options_page": "panel.html"
}
